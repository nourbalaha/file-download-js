{
  "name": "file-download-js",
  "version": "0.0.1",
  "description": "An npm package to download files using javascript",
  "main": "index.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nourbalaha/file-download-js.git"
  },
  "keywords": [
    "file-downloader-js",
    "file-downloader-ts",
    "js-file-downloader",
    "ts-file-downloader",
    "file-download-js",
    "file-download-ts",
    "js-file-download",
    "ts-file-download",
    "file-downloader",
    "file-download"
  ],
  "author": "Nour Balaha",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nourbalaha/file-download-js/issues"
  },
  "homepage": "https://github.com/nourbalaha/file-download-js#readme",
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "jest": "^29.2.2",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.4"
  },
  "files": [
    "lib/**/*"
  ]
}